<%- include('partials/header') -%>

<h1>Home</h1>
<p> <%= startingContent %> </p>

<% posts.forEach(post => { %>
<div class = "post-preview">
    <h2><%= post.title %></h2>
    <p><%= post.body.substring(0, 100) + " ..." %> <a href="posts/:<%= post.title %> ">Read More</a></p>
</div>
<% }); %>


<%- include('partials/footer') -%>